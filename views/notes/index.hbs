<h1>All Notes</h1>

{{#each toolNote}}
<div class="row">
    <div class="col s16 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title">{{createdAt}}</span>
                <p>{{noteContent}}</p>
            </div>

        </div>
    </div>
</div>

{{/each}}