<h1>All Notes</h1>

{{#each toolNotes}}
<div class="row">
    <div class="col s16 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title">{{createdAt}}</span>
                <p>{{noteContent}}</p>
            </div>

        </div>
    </div>
</div>
<form action="/{{toolNote._id}}?_method=DELETE" method="POST">
    <button class="btn" type="submit">Delete</button>
</form>

{{/each}}